#, php-format
msgid "%d day"
msgid_plural "%d days"
msgstr[0] "%d päivä"
msgstr[1] "%d päivää"

#: earth-hour.php:76
#, php-format
msgid "%d hour"
msgid_plural "%d hours"
msgstr[0] "%d tunti"
msgstr[1] "%d tuntia"

#: earth-hour.php:77
#, php-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d minuutti"
msgstr[1] "%d minuuttia"

#: earth-hour.php:79
#, php-format
msgid "<a href=\"http://www.bravenewcode.com/earth-hour/\" rel=\"nofollow\">Earth Hour</a> begins in %1$s, %2$s, and %3$s"
msgstr "<a href=\"http://www.bravenewcode.com/earth-hour/\" rel=\"nofollow\">Earth Hour</a> -tapahtuman alkuun aikaa %1$s, %2$s ja %3$s"

#: html/message.php:10
msgid "Proudly Supporting Earth Hour"
msgstr "Earth Hour -tapahtuman ylpeä tukija"

#: html/message.php:16
#, php-format
msgid "%s is currently participating in"
msgstr "%s on tällä hetkellä mukana tapahtumassa"

#: html/message.php:17
#, php-format
msgid "There is currently %d other WordPress site supporting this cause."
msgid_plural "There are currently %d other WordPress sites supporting this cause."
msgstr[0] "Tapahtumaa tukee %d muu WordPress-sivu."
msgstr[1] "Tapahtumaa tukee %d muuta WordPress-sivua."

#: html/message.php:18
msgid "If you'd like to join us, download the <br /><a href=\"http://www.bravenewcode.com/earth-hour/\">WordPress Earth Hour plugin</a>"
msgstr "Jos haluat liittyä mukaan, lataa <br /><a href=\"http://www.bravenewcode.com/earth-hour/\">WordPress Earth Hour -lisäosa</a>"

#: html/message.php:19
#, php-format
msgid "This website will be back online in %d minute."
msgid_plural "This website will be back online in %d minutes."
msgstr[0] "Tämä verkkosivu palaa takaisin toimintaan %d minuutin päästä."
